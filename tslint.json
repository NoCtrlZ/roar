{
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
      "prettier": true,
      "quotes": [2, "single"],
      "no-unused-vars": ["error", { "vars": "local" }],
      "no-undef": "error",
      "no-prototype-builtins": 0
    }
}
